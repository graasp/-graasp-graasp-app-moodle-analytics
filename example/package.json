    "@material-ui/icons": "4.9.1",
    "eslint": "link:../node_modules/eslint",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "env-cmd": "^10.1.0",
    "json-server": "^0.16.3",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",

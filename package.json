{
  "name": "@graasp/moodle",
  "version": "0.1.3",
  "description": "Components for connecting Graasp with Moodle.",
  "author": "graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Roland Salloum",
    "Hady Khairallah"
  ],
  "license": "MIT",
  "repository": "https://github.com/graasp/graasp-moodle",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "prepare": "run-s build",
    "test": "run-s test:unit lint test:build",
    "test:build": "run-s build",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,example}/**/*.js'",
    "prettier:write": "prettier --write '{src,example}/**/*.js'",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release"
  },
  "peerDependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@nivo/bar": "0.62.0",
    "@nivo/legends": "0.62.0",
    "d3-scale-chromatic": "2.0.0",
    "dextrous": "0.2.7",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "i18next": "17.0.16",
    "lodash": "4.17.19",
    "moment": "2.27.0",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react-day-picker": "7.4.8",
    "react-ga": "2.7.0",
    "react-helmet": "6.1.0",
    "react-i18next": "10.12.5",
    "react-moment": "0.9.7",
    "react-redux": "7.2.0",
    "react-scripts": "4.0.1",
    "react-select": "3.1.0",
    "react-toastify": "5.5.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-promise": "0.6.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "codacy-coverage": "3.4.0",
    "cross-env": "7.0.2",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "gh-pages": "2.2.0",
    "glob": "7.1.6",
    "husky": "4.2.5",
    "json-server": "0.16.1",
    "microbundle-crl": "0.13.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.4",
    "pretty-quick": "2.0.1",
    "standard-version": "9.0.0"
  },
  "files": [
    "dist"
  ]
}
